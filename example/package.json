{
  "name": "example",
  "version": "0.1.0",
  "main": "src/index.ts",
  "description": "This is a example",
  "scripts": {
    "lint": "biome check --write src/",
    "service": "nodemon",
    "start": "ts-node src/index.ts",
    "release": "standard-version",
    "test:local": "jest --setupFiles dotenv/config --ci -i --forceExit",
    "test:ci": "jest --ci -i"
  },
  "author": "AnthonyLzq <sluzquinosa@uni.pe>",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "dotenv": "^17.2.1",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "nodemonConfig": {
    "watch": [
      ".env",
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "src/**/*.test.ts"
    ],
    "exec": "npx ts-node -r dotenv/config ./src/index"
  }
}
